{
  "name": "straschek-io/typo3-hyphenator",
  "description": "Provides soft-hyphen replacement for TYPO3",
  "license": "GPL-2.0-or-later",
  "support": {
    "email": "hallo@straschek.io",
    "source": "https://github.com/straschek-io/typo3-hyphenator",
    "issues": "https://github.com/straschek-io/typo3-hyphenator/issues"
  },
  "authors": [
    {
      "name": "Michael Straschek",
      "email": "hallo@straschek.io"
    }
  ],
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "StraschekIo\\Hyphenator\\": "Classes/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.27"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "typo3_hyphenator"
    }
  }
}
